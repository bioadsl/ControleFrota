<?xml version="1.0" encoding="UTF-8"?>
<!-- arquivo web.xml -->
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:javaee="http://xmlns.jcp.org/xml/ns/javaee" 
	xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" 
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  
  <servlet>
    <servlet-name>ServletCasdastrar</servlet-name>
    <servlet-class>br.com.frota.controller.ServletCasdastrar</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletCasdastrar</servlet-name>
    <url-pattern>/cadastrar</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ServletEditar</servlet-name>
    <servlet-class>br.com.frota.controller.ServletEditar</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletEditar</servlet-name>
    <url-pattern>/alterar</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>ServletConsulta</servlet-name>
    <servlet-class>br.com.frota.controller.ServletConsulta</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletConsulta</servlet-name>
    <url-pattern>/consulta</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ServletExcluir</servlet-name>
    <servlet-class>br.com.frota.controller.ServletExcluir</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletExcluir</servlet-name>
    <url-pattern>/excluir</url-pattern>
  </servlet-mapping>

</web-app>