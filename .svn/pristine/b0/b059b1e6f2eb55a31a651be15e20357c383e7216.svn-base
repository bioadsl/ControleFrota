	background-image: -moz-linear-gradient(top,  #afda1c,  #8aba16); /* for firefox 3.6+ */ 
    background-color: #AFDA1C;
    border-color: #9cb956 #90a953 #829a48;
}

.entry blockquote.social .from.facebook span{
	background: url("../img/sprite.png") no-repeat scroll -76px -81px transparent;
    height: 20px;
    margin: 6px;
    padding: 10px;
    width: 20px;
}

.entry blockquote.social .facebook{
    background: #3f609e; /* for non-css3 browsers */
	background-image: -webkit-gradient(linear, left top, left bottom, from(#4a70ae), to(#34508f)); /* for webkit browsers */
	background-image: -moz-linear-gradient(top,  #4a70ae,  #34508f); /* for firefox 3.6+ */ 
    border-color: #3b5a8c #345085 #2f487e;
}

.entry blockquote.social .from.twitter span{
	background: url("../img/sprite.png") no-repeat scroll -109px -81px transparent;
    height: 20px;
    margin: 6px;
    padding: 10px;
    width: 20px;
}

.entry blockquote.social .twitter{
    background: #3f609e; /* for non-css3 browsers */
	background-image: -webkit-gradient(linear, left top, left bottom, from(#2ebdeb), to(#269dc3)); /* for webkit browsers */
	background-image: -moz-linear-gradient(top,  #2ebdeb,  #269dc3); /* for firefox 3.6+ */ 
    border-color: #10a8db #0895c4 #0183af;
}

/* TABLE */
.entry table {
    border: solid 1px #e5e5e5;
    width: 100%;
    margin: 0 0 24px 0;
    font-size: 0.8em;
	font-family: "Lucida Grande", "Lucida Sans", Tahoma, sans-serif;
}

.entry table th, .entry table td {
    border: solid 1px #e5e5e5;
    padding: 5px 14px;
}
.entry table th {
    background-color: #f4f4f4;
    font-weight: bold;
}

/* CAPTION EM IMAGENS */
.wp-caption {
    background-color: #f3f3f3;
    border: 1px solid #ddd;
    margin: 10px;
    padding: 4px;
	font-family: "Lucida Grande", "Lucida Sans", Tahoma, sans-serif;
    text-align: center;
}
.wp-caption p.wp-caption-text {
    margin: 0;
    padding: 4px 0;
    font-size: 0.7em;
    line-height: 1.3em;
}

/* T�PICOS RELACIONADOS E TAGS */
.after-post {
    float: left;    
    width: 100%;
    line-height: 1.3em;
    font-size: 1.3em;
}

.after-post .box {
    margin-left: 20px;
    overflow: hidden;
    background-color: #f4f4f4;    
    border: solid 1px #e5e5e5;
}

.after-post h5 {
	margin-bottom: 10px;
    font-weight: bold;
    color: #666;
}

.after-post .related {
    float: left;
    width: 54%;
    padding: 14px 10px 0 18px;
}

.after-post .related ul li{
	margin-bottom: 10px;
}

.after-post .related a:hover {
    text-decoration: underline;
}
.after-post .tags {
    float: left;
    width: 37%;
    padding: 14px 18px 10px 10px;
}

.after-post .tags a {
    float: left;
    display: block;
    padding: 6px 8px;
    margin: 0 6px 6px 0;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    background-color:#fff;
    color: #7caa0b;
    white-space:nowrap;
}

.after-post .tags a:hover {
    color: white;
    background: #AFDA1C; /* for non-css3 browsers */
	background-image: -webkit-gradient(linear, left top, left bottom, from(#afda1c), to(#8aba16)); /* for webkit browsers */
	background-image: -moz-linear-gradient(top,  #afda1c,  #8aba16); /* for firefox 3.6+ */  
}

/* COMPARTILHAMENTO E IMPRESS�O */
.tools_box {
    float: left;    
    width: 100%;
    line-height: 1.8em;
    font-size: 1.3em;
    color: #3C3C3C;
}

.tools_box a:hover {
	text-decoration: underline;
}

.tools_box .box 
{
	margin-top: 16px;
    padding-left: 20px;
    border-left: 4px solid #9bd01a;
    overflow: hidden;
}

.tools_box .box_gray
{
    padding: 12px 18px;
    border: 1px solid #DEDEDE;
    overflow: hidden;
    background-color: #F1F1F1;
}

.tools_box .social {
    width: 100%;
    padding: 0 0 4px;
    border-bottom: solid 1px #dedede;
    overflow: hidden;
 }

.tools_box .social span{
    float: left;
    margin-right: 10px;
}

.tools_box .tools {
    float: right;
    overflow: hidden;
    margin-top: 3px;
    height: 25px;
}

.tools_box .tools a {
    display: block;
    height: 16px;
    width: 16px;
}

.tools_box .tools span {
    float: right;
    margin-left: 5px;
}

.tools_box .tools a.print {
    background: url('../img/sprite.png') no-repeat -59px -135px;
}

.tools_box .tools a:hover.print {
    background: url('../img/sprite.png') no-repeat -59px -153px;
}

.tools_box .tools a.pdf {
    background: url('../img/sprite.png') no-repeat -42px -135px;
}

.tools_box .tools a:hover.pdf {
    background: url('../img/sprite.png') no-repeat -42px -153px;
}

.tools_box .info {
    float: left;
    width: 100%;    
    padding: 8px 0 0;
	line-height: 1.3em;
    overflow: hidden;    
}

/* AUTHOR BOX */
.author-post {
    float: left;
    width: 100%;
    line-height: 1.8em;
    font-size: 1.3em;
    color: #666;
}

.author-post a:hover {
	text-decoration: underline;
}

.author-post .box {
    margin: 16px 0 0 20px;
    padding: 16px 18px;
    background-color: #f4f4f4;    
    border: solid 1px #dedede;
    overflow: hidden;
}

.author-post img 
{
    float: left;
    padding: 5px;
	margin: 0 18px 8px 0;
    border: solid 1px #dedede;
    background-color: #fff;    
}

.author-post .desc {
    margin-left: 124px;
}

.author-post .desc ul 
{
    margin: 8px 0 0 8px;
}

.author-post .desc ul li 
{
    display: inline;
    margin-right: 10px;
}

.author-post .recent 
{
    float: left;
    clear: both;    
    width: 100%;
    margin: 8px 0 0 0;
    padding-top: 10px;
    background-color: #f4f4f4;    
    border-top: solid 1px #dedede;
}

.author-post .recent ul
{
    list-style-type: square;
    list-style-position: inside;
}

/* LISTA DE COMENT�RIOS */
.comments {
    float: left;
    width: 100%;
    line-height: 1.8em;
    font-size: 1.3em;
    color: #424242;
}

.comments .box {
    margin: 16px 0 0 20px;
    overflow: hidden;
}

.comments h3 {
    color: #666666;
    line-height: 1.7em;
    font-size: 1.7em;
    padding: 0 0 6px;
}

.comments ul li 
{
    padding: 14px 18px;
    background-color: #f4f4f4;
    overflow: hidden;
}

.comments .alt
{
