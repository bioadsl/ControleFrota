/* CSS Compre Mais!!! */

/*--- CSS RESET ---*/
html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, embed, 
figure, figcaption, footer, header, hgroup, 
menu, nav, output, ruby, section, summary,
time, mark, audio, video {
    margin: 0;
    padding: 0;
    border: 0;
    font-size: 100%;
    font: inherit;
    vertical-align: baseline;
}
/* HTML5 display-role reset for older browsers */
article, aside, details, figcaption, figure, 
footer, header, hgroup, menu, nav, section {
    display: block;
}
body {
    line-height: 1;
}
ol, ul {
    list-style: none;
}
blockquote, q {
    quotes: none;
}
blockquote:before, blockquote:after,
q:before, q:after {
    content: '';
    content: none;
}
table {
    border-collapse: collapse;
    border-spacing: 0;
}
/*--- END: CSS RESET ---*/

/*--- CLASS GERAL ---*/

body{
    font-family: "Lucida Grande", "Lucida Sans", Tahoma, sans-serif;
    color: #424242;
    font-size: 62.5%; /* Reduz os 16px padrão para 10px e facilita os cálculos */
}

.inner {
    width: 980px; /* 980px ou 78% */
    /*min-width: 800px;
    max-width: 980px;*/
    margin: 0 auto 0;
    padding: 0 6px;
}

.inner > h2 { /* BUSCA GOOGLE */
    margin: -12px 0 32px 0;
	font-size: 2.2em;
    line-height: 1.9em;	
    color: #424242;
	border-bottom: solid 1px #f1f1f1;
}

a, 
a:hover
{
    text-decoration: none;
    color: #7caa0b;
}

strong { 
    font-weight: bold;
}

em {
    font-style: italic;
}

.radiuscorner {
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius: 5px; 
}

::-moz-selection {
	color: #fff;
	background: #9bd01a;
}
::selection {
	color: #fff;
	background: #9bd01a;
}
/*--- END: CLASS GERAL ---*/

/*--- ADSTOP ---*/
#adstop_wrapper{
    width: 100%;
    background-color: #9bd01a;
}

#adstop {
    overflow: hidden;
}
/*--- END: ADSTOP ---*/

/*--- HEADER ---*/
#header_wrapper {
    width: 100%;
    border-top: 5px solid #9bd01a;
    background-color: #333333;
}

#header {
    overflow: hidden;
}

#header h1 {
    float:left;
}

#header h1 a {
	display: block;
	margin: -2px 8px 4px 0;
	background: url('../img/compre_mais_logo.png') no-repeat 0 0;
	width: 292px;
	height: 78px;
	text-indent: -5000px;
	overflow: hidden;
}

#header .slogan {
    float: left;
    max-width: 450px;
}

#header .slogan p 
{
    margin: 21px 0 21px 8px;
    padding: 6px 0 5px 15px;
    border-left: solid 1px #5b5b5b;
    line-height: 1.3em;
    font-size: 1.28em;
    color: #b5b5b5;
    text-shadow: 0 1px 0 #272727;
}

#header ul {
    float: right;
}

#header ul li {
    float: left;
}

#header ul li a {
    display: block;
    margin: 20px 0 10px 6px;
    padding: 10px 8px;
    font-size: 1.1em;
	line-height: 1em;
    font-weight: bold;
    letter-spacing: 0.5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius: 5px; 
    color: #999999;
    text-shadow: 0 1px 0 #272727;
}

#header ul li a.dropdown {
	background-color: #404040; 
}

#header ul li a:hover {
    color: #d1d1d1;
    background-color: #404040; 
}

#header ul li .icon {
    float: left;
    width: 18px;
	height: 18px;
	margin: -3px 4px 0 0;
}

#header ul li .seta {
	font-size: 10px !important;
	font-family: arial;
}

#header ul li .idiomas {
    background: url('../img/sprite.png') no-repeat -7px -63px;
}

#header ul li .cursos {
    background: url('../img/sprite.png') no-repeat -7px -94px;
}

#header ul li .store {
    background: url('../img/sprite.png') no-repeat -7px -126px;
}

#header .menu ul {
	position: absolute;
	display: none;
    width: 150px;
	margin: -13px 0 0 6px;	
	z-index: 9999;
	background-color: #404040;
	-moz-box-shadow: 2px 4px 5px rgba(0, 0, 0, 0.32);
	 -webkit-box-shadow: 2px 4px 5px rgba(0, 0, 0, 0.32);
	 box-shadow: 2px 4px 5px rgba(0, 0, 0, 0.32);	
}

#header .menu ul li {
	width: 100%;
}

#header .menu ul li a {
	display: block;
	padding: 10px;
	margin: 0;
	border-bottom: 1px solid #666;
    -moz-border-radius: 0;
    -webkit-border-radius: 0;
    border-radius: 0;	
}

#header .menu li:hover ul {
	display: block;
}

/*--- END: HEADER ---*/

/*--- MENU GERAL ---*/

#menu_wrapper {
    width: 100%;
    margin-bottom: 18px;
    background-color: #f4f4f4;
    border-bottom: 1px solid #e1e1e1;
    -webkit-box-shadow: 0px 0px 1px 0px rgba(0, 0, 15, 0.1), inset 0 0 0 1px white;
	-moz-box-shadow: 0px 0px 1px 0px rgba(0, 0, 15, 0.1), inset 0 0 0 1px white;
	box-shadow: 0px 0px 1px 0px rgba(0,0,0,0.15), inset 0 0 0 1px white;
}

#menu {
    overflow: hidden;
}

#menu ul {
    float: left;
    border-left: 1px solid #e1e1e1;
}

#menu ul li {
    float: left;
    border-right: 1px solid #e1e1e1;
}

#menu ul li a {
    display: block;
    padding: 14px 20px;
    font-size: 1.2em;
    font-weight: bold;
    color: #676767;
}

#menu ul li a .seta {
	font-size: 10px !important;
}

#menu ul li a:hover{
	background: #AFDA1C; /* for non-css3 browsers */
	background-image: -webkit-gradient(linear, left top, left bottom, from(#afda1c), to(#8aba16)); /* for webkit browsers */
	background-image: -moz-linear-gradient(top,  #afda1c,  #8aba16); /* for firefox 3.6+ */ 
	color:#fff;
	text-shadow: 0 1px 0 #749708;
}

#menu .menu ul{
	position: absolute;
	display: none;
    width: 240px;
	border-top: 1px solid #dedede;
	z-index: 9999;
	-moz-box-shadow: 2px 4px 5px rgba(0, 0, 0, 0.32);
	 -webkit-box-shadow: 2px 4px 5px rgba(0, 0, 0, 0.32);
	 box-shadow: 2px 4px 5px rgba(0, 0, 0, 0.32);
}

#menu .menu ul li {
	width: 100%;
	border-bottom: 1px solid #dedede;
    background-color: #f4f4f4;
}

#menu .menu li:hover ul{
	display: block;
}

/* CAIXA DE BUSCA */
.search_box {
    float: right;
}

.search_box span {
    float: left;
    margin: 12px 8px 0 0;
    font-size: 1.2em;
    line-height: 1.4em;	
    font-weight: bold;
    color: #8f8d8d;
}

.search_box form {
    float: left;
    margin: 7px 0 6px;
}

.search_box form .text,
.search_box form .textwp {
    float: left;
    width: 214px;
    height: 20px;
	padding: 2px !important;    
	margin: 0;
	font-size: 15px !important;
	line-height: 19px !important;
	border: solid 1px #e5e5e5 !important;
}

.search_box form .textwp {
	background: #fff;
}

.search_box form .submit{
    float: left;
    display: block;   
    width: 48px;
    height: 26px;
    margin: 0;
    border: solid 1px #e5e5e5;
    border-left: 0;
    background: url('../img/sprite.png') no-repeat -144px -123px #F7F7F7;
    -moz-border-radius: 0 5px 5px 0;
    -webkit-border-radius: 0 5px 5px 0;
    border-radius: 0 5px 5px 0;
}

.search_box form .submit:hover{
    background-position: -144px -149px;
    background-color: #AFDA1C;
}

/*--- END: MENU GERAL ---*/

/*--- CONTENT ---*/
#content_wrapper {
    float: left;
    width: 100%;
    margin-left:-330px;
}

#content {
    margin-left: 330px;
    overflow:hidden;
}

/* BREADCRUMB */
.breadcrumb,
.featured  {
    width: 100%;
	padding-bottom: 10px;
    line-height: 1.4em;
    font-size: 1.4em;
    border-bottom: solid 1px #f1f1f1;	
}

.breadcrumb a:hover
{
    text-decoration: underline;
}

/* FEATURED */
.featured {
	font-weight: bold;
    text-transform: uppercase;
	color: #7caa0b;
}

/* TÍTULO DO POST, PÁGINAS E ARQUIVOS */
.title {
    padding-left: 16px;
    border-left: 4px solid #9bd01a;
    margin: 16px 0;
}

.title h2 {
    line-height: 1.2em;
    font-size: 2.3em;
    font-weight: normal;
    color: #424242;
}

.title h2 a {
    color: #424242;
}

.title .postmeta {
    line-height: 1.3em;
    font-size: 1.23em;
    margin-top:4px;
    color: #999999;
}

.title .postmeta a {
    color: #999;
}

.title a:hover{
    background-color: #9bd01a;
    color: #fff;
}

/* TÍTULO DAS CATEGORIAS */
.category{
    float: left;
    padding: 10px 0 12px 0;
    width: 100%;
    background: #AFDA1C; /* for non-css3 browsers */
	background-image: -webkit-gradient(linear, left top, left bottom, from(#afda1c), to(#8aba16)); /* for webkit browsers */
	background-image: -moz-linear-gradient(top,  #afda1c,  #8aba16); /* for firefox 3.6+ */
	margin-bottom: 12px;
}

.category h2 {
    margin: 0 24px;
    line-height: 1.6em;
    font-size: 2.2em;
    color: #FFF;
}

.category .desc 
{
    margin: 0 24px;
    line-height: 2em;
    font-size: 1.4em;
    color: #547406;
}

/* CONTE�DO DO POST */
.entry {
    clear: both;
    padding: 8px 36px 0 18px;
    font-family: Georgia, "Times New Roman", Times, serif;
    font-size: 1.65em; /* CUIDADO COM O IEca */
    line-height: 1.65em;
}

.entry p {
    margin: 0 0 24px 0;
	color:#424242;
}

.entry a {
	border-bottom: solid 1px #7caa0b;
	color: #424242;
}

.entry a:hover {
	background-color: #9bd01a;
	color: #fff;
}

.entry img {
    margin: 4px;
}

.entry .excerpt p{
    font-size: 1.2em;
    line-height: 1.4em;
    color: #999;
}

/* NOTAS LATERAIS */
.entry .aside
{
    float: left;
    width: 160px;
    margin: 6px 18px 20px 0;
    padding: 12px 18px;
    text-align: right;
	font-family: "Lucida Grande", "Lucida Sans", Tahoma, sans-serif;
    font-size: 0.78em;
    line-height: 1.4em;
    z-index: 9999;
    background-color: #F4F4F4;
	color: #666;
}

.entry .aside.alignright
{
    float: right;
    margin: 0 0 20px 18px;
    text-align: left;
}

/* ALINHAMENTO */
.entry .alignleft {
    float: left;
    margin: 6px 18px 16px 0;
}

.entry .alignright {
    float: right;
    margin: 6px 0 16px 18px;
}

.entry .aligncenter {
    float: none;
    margin: 18px;
}

.entry .ads, 
.arquive  .ads {
	float: right;
    display: block;    
    width: 300px;
    height: 250px;    
    border: solid 1px #e5e5e5;
    background-color: #f4f4f4;
	margin: 4px -34px 16px 16px;
}

.arquive  .ads { 
	margin: 4px 0 16px 16px;
}

.entry .ads.left, 
.arquive  .ads.left{
	float: left;
	margin: 4px 0 16px -24px;
}

.arquive  .ads.left{
	margin: 4px 0 16px 0;
}

/* SUBT�TULO */
h4 {
    clear: both;
    margin: 0 0 14px 0;
	padding: 0 0 8px 0;
    border-bottom: 1px solid #e5e5e5;
    line-height: 1.2em;
    font-family: "Lucida Grande", "Lucida Sans", Tahoma, sans-serif;
    font-size: 1.3em;
	font-weight: normal;
}

/* LISTAS */
.entry ul 
{
    list-style-type: square;    
}

.entry ol 
{
    list-style: decimal;
}

.entry ul, .entry ol {
    padding: 0 0 15px 15px;
    list-style-position: outside;
}

/* CITA��ES */
.entry blockquote {
    padding-left: 16px;
    border-left: 4px solid #e5e5e5;
    font-style: italic;
    font-size: 0.9em;
}

.entry blockquote p {
	color: #747474;
}

.entry blockquote.social 
{
	background: url("../img/sprite.png") no-repeat scroll -184px -373px transparent;
	border-left: 1px solid #DEDEDE;
	border-right: 1px solid #DEDEDE;
	border-top: 1px solid #DEDEDE;
	margin-bottom: 24px;
	padding: 0 0 10px;
}

.entry blockquote.social p {
    padding: 10px 30px 10px 50px;
    margin: 0;
}

.entry blockquote.social p.author 
{
    background: #f4f4f4;
    padding: 0 0 0 66px;
    margin: 0;
    font-weight: bold;
	font-family: "Lucida Grande", "Lucida Sans", Tahoma, sans-serif;
    font-style: normal;
    font-size: 0.8em;
	line-height: 1.6em;
